{
  "name": "azure-staticwebapp-vite",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "run-p 'dev:*'",
    "dev:swa": "swa start ./public --api-location server/ --port 3000 --api-devserver-url http://127.0.0.1:3030",
    "dev:function": "func start --prefix server/ --cors-credentials --port 3030",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "deploy": "swa deploy"
  },
  "dependencies": {
    "@azure/functions": "^4.6.0",
    "@remix-run/react": "^2.15.2",
    "@scandinavianairlines/remix-azure-functions": "^1.0.3",
    "isbot": "^5.1.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.2",
    "@remix-run/dev": "^2.15.2",
    "@remix-run/eslint-config": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "azure-functions-core-tools": "^4.0.6610",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "vite": "^6.0.7"
  },
  "resolutions": {
    "axios": ">=1.7.4",
    "braces": ">=3.0.3",
    "cookie": ">=0.7.0",
    "cross-spawn": ">=7.0.5",
    "got": ">12.1.0",
    "nanoid": ">=3.3.8",
    "path-to-regexp": ">=0.1.12",
    "tar": ">=6.2.1",
    "undici": ">=5.28.4",
    "ws": ">= 7.5.10"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
