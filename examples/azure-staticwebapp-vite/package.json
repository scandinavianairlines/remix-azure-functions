{
  "name": "azure-staticwebapp-vite",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "run-p 'dev:*'",
    "dev:swa": "swa start ./public --api-location server/ --port 3000 --api-devserver-url http://127.0.0.1:3030",
    "dev:function": "func start --prefix server/ --cors-credentials --port 3030",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "deploy": "swa deploy"
  },
  "dependencies": {
    "@azure/functions": "^4.4.0",
    "@remix-run/react": "^2.8.1",
    "@scandinavianairlines/remix-azure-functions": "^1.0.1",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.1.7",
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "azure-functions-core-tools": "^4.0.5611",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "vite": "^5.2.8"
  },
  "resolutions": {
    "axios": ">=0.28.0",
    "got": ">12.1.0",
    "undici": ">=5.28.4",
    "tar": ">=6.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
