{
  "name": "@examples/azure-staticwebapp",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "run-p 'dev:*'",
    "dev:swa": "swa start ./public --run \"remix watch\" --api-location server/ --port 3000 --api-devserver-url http://127.0.0.1:3030",
    "dev:function": "func start --prefix server/ --cors-credentials --port 3030",
    "deploy": "swa deploy ",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@scandinavianairlines/remix-azure-functions": "^1.0.1",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.1.6",
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "azure-functions-core-tools": "^4.0.5455",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
